   "source": [
    "function fib(n::Int64)\n",
    "    if n == 0\n",
    "        return 0\n",
    "    elseif n==1 || n==2 \n",
    "        return 1\n",
    "    else \n",
    "        return fib(n-1) + fib(n-2)\n",
    "    end"
   ]
  }
